@model WebCrawler.Models.Interval
@{
    var time = ViewBag.time;
}

<body>
    <script  type="text/javascript">
        var a = "Run_DataProcessing"
        setTimeout("location.href = a", @time*1000)
    </script>

    
    <h5>下一次執行秒數 @time 秒</h5>
</body>


